<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cyy.interthread">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RunOnUiTestActivity"/>
        <activity android:name=".PostTestActivity"/>
        <activity android:name=".PostDelayTestActivity"/>
        <activity android:name=".HandlerTestActivity"/>
        <activity android:name=".AsyncTaskTestActivity"/>
    </application>

</manifest>